<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Open URLs</title>
</head>
<body>
    <button onclick="openURLs()">Open URLs</button>

    <script>
        // Function to open URLs in a new tab
        const openURLs = () => {
            const urlsToOpen = [
                'http://radon.games/uv/~/hvtrs8-wuw,pmrlhwb,cmm-vkdgo-sgapcj?qecrah?hgnvak+cnkmg',
                'http://radon.games/uv/~/hvtrs8-wuw,pmrlhwb,cmm-vkdgo-sgapcj?qecrah?hgnvak+cnkmg',
                'http://radon.games/uv/~/hvtrs8-wuw,pmrlhwb,cmm-vkdgo-sgapcj?qecrah?hgnvak+cnkmg'
            ];

            urlsToOpen.forEach((url) => window.open(url, '_blank'));
        };
    </script>
</body>
</html>
